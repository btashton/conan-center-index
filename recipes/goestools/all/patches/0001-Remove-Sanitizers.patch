 8 files changed, 12 deletions(-)

diff --git a/CMakeLists.txt b/CMakeLists.txt
index d5edf1e..528ab74 100644
--- a/CMakeLists.txt
+++ b/CMakeLists.txt
@@ -32,9 +32,6 @@ if(${CMAKE_SYSTEM_PROCESSOR} MATCHES "^arm")
   endif()
 endif()
 
-set(CMAKE_MODULE_PATH ${CMAKE_SOURCE_DIR}/vendor/sanitizers-cmake/cmake ${CMAKE_MODULE_PATH})
-find_package(Sanitizers)
-
 # Force static build of libaec
 set(BUILD_SHARED_LIBS OFF CACHE INTERNAL "" FORCE)
 
diff --git a/src/dcs/CMakeLists.txt b/src/dcs/CMakeLists.txt
index f2780d8..9f77858 100644
--- a/src/dcs/CMakeLists.txt
+++ b/src/dcs/CMakeLists.txt
@@ -1,5 +1,4 @@
 add_library(dcs dcs.cc)
 
 add_executable(dcsdump dcsdump.cc)
-add_sanitizers(dcsdump)
 target_link_libraries(dcsdump lrit dcs m stdc++)
diff --git a/src/decoder/CMakeLists.txt b/src/decoder/CMakeLists.txt
